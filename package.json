{
  "name": "rugxfit",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:migrate": "dotenv -e .env -- prisma migrate dev",
    "db:push": "dotenv -e .env -- prisma db push",
    "db:seed": "dotenv -e .env -- node --loader ts-node/esm prisma/seed.ts",
    "db:studio": "dotenv -e .env -- prisma studio",
    "db:generate": "dotenv -e .env -- prisma generate",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.0.8",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "next": "14.2.14",
    "radix-ui": "^1.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^2.2.1",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.18",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.14",
    "postcss": "^8.4.35",
    "prisma": "^5.10.2",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
